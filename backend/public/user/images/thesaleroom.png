			function submitNewsletter() {

				jQuery('#errorMessageContainerId').hide();
				if(isValidEmailAddress(jQuery('#control_EMAIL').val()) && jQuery('#control_EMAIL').val() != '' && jQuery('#cb_firstname').val() != '' && jQuery('#cb_surname').val() != ''){
					if (!formSubmit) {
						jQuery.ajax({
							url: jQuery('#base_url').val(),
							type: 'POST',
							data: {
								option: 'com_myaccount',
								task: 'setSignUpEmail',
                                signup_email_address: jQuery('#control_EMAIL').val(),
                                FName: jQuery('#cb_firstname').val(),
								LName: jQuery('#cb_surname').val()
							},
						}).done(function(response) {
							formSubmit = true;
							jQuery('#signup-form-top').submit();
						});

						return formSubmit;
					}
				} else {
					jQuery('#errorMessageContainerId').show();
					alert('All fields are required');
					return false;
				}
			}

            function isValidEmailAddress(emailAddress) {
                var pattern = new RegExp(/^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/i);
                return pattern.test(emailAddress);
            };
